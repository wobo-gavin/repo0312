<?xml version="1.0" encoding="UTF-8"?>
<phpunit bootstrap="./bootstrap.php"
         colors="true"
         processIsolation="false"
         stopOnFailure="false"
         syntaxCheck="false"
         convertErrorsToExceptions="true"
         convertNoticesToExceptions="true"
         convertWarningsToExceptions="true"
         testSuiteLoaderClass="PHPUnit_Runner_StandardTestSuiteLoader">

    <testsuites>
        <testsuite name="/* Replaced /* Replaced /* Replaced Guzzle */ */ */">
            <directory>./</directory>
        </testsuite>
    </testsuites>

    <!-- Ignore these files in code coverage analysis -->
    <filter>
        <whitelist>
            <directory suffix=".php">./../../../library//* Replaced /* Replaced /* Replaced Guzzle */ */ */</directory>
        </whitelist>
    </filter>

</phpunit>